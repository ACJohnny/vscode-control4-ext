{
  "extends": "@tsconfig/svelte/tsconfig.json",
  "display": "Svelte",
  "include": [
    "./**/*",
  ],
  "exclude": [
    "../node_modules/*"
  ],
  "compilerOptions": {
    "moduleResolution": "node",
    "target": "es2017",
    "sourceMap": true,
    "strict": false,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "ignoreDeprecations": "5.0",
  }
}